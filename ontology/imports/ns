        <rdfs:isDefinedBy rdf:resource="http://rdfs.org/sioc/ns#"/>
    </owl:Class>
    


    <!-- http://rdfs.org/sioc/ns#Forum -->

    <owl:Class rdf:about="&sioc;Forum">
        <rdfs:label xml:lang="en">Forum</rdfs:label>
